
<form action="/landmarks/<%= @landmark.id %>" method='post'>

  <input type="hidden" name="_method" value="patch">
  <%= @landmark.year_completed %>
  Landmark Name <input type="text" id="name" name="landmark[name]" value="<%= @landmark.name%>"/><br/>
  Landmark Year Completed <input type="text" id="year_completed" name="landmark[year_completed]" value="<%= @landmark.year_completed.to_s %>"/><br/>

  <input type='submit' value='Edit Landmark'/>
</form>
